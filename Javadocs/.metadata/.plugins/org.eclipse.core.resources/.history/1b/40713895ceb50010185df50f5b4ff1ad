package ejercicio8;

import java.util.Scanner;

public class Ejercicio8 {

	public static void main(String[] args) 
	{
		Scanner teclado= new Scanner(System.in);
		int numero;
		
		do
		{
			System.out.println("Dame numeros nulos (poner 0 para salir)");
			 numero=teclado.nextInt();
		} while(numero==0);
	}

}
